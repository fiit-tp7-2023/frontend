{
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true,
    "source.organizeImports": "never"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "cSpell.words": [
    "FIIT",
    "Josefin",
    "Lato",
    "nuxt",
    "nuxtjs",
    "Raleway",
    "Roboto",
    "sidebase",
    "tailwindcss",
    "vueuc",
    "wght"
  ]
}
